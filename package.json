{
  "name": "@ar-io/exp-engine",
  "version": "0.0.1",
  "description": "Pulls stats related to ar.io experience points from onchain and quest platform.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "dependencies": {
    "@ar.io/sdk": "^3.8.1",
    "axios": "^0.26.0",
    "axios-retry": "^3.2.4",
    "country-list": "^2.3.0",
    "csv-parser": "^3.1.0",
    "csv-writer": "^1.6.0",
    "geoip-lite": "^1.4.10",
    "mathjs": "^13.2.0",
    "node-cron": "^3.0.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.7.0",
    "@permaweb/aoconnect": "^0.0.45",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/country-list": "^2.1.4",
    "@types/geoip-lite": "^1.4.4",
    "@types/jest": "^27.4.0",
    "@types/node": "^18.15.7",
    "ajv": "^8.12.0",
    "commitlint": "^18.2.0",
    "copyfiles": "^2.4.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.17.12",
    "eslint": "^6.7.2",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^8.0.3",
    "inquirer": "8.0.0",
    "jest": "^27.4.3",
    "jest-junit": "^16.0.0",
    "lint-staged": "^14.0.1",
    "prettier": "^2.2.1",
    "replace-in-file": "^6.2.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.9.1",
    "typescript": "4.3.5"
  },
  "scripts": {
    "build": "yarn run clean && yarn run build-ts && npm run cp",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "build-ts": "tsc",
    "clean": "rimraf ./dist",
    "cp": "copyfiles -u 1 ../lib/**/*.json dist",
    "prepare": "yarn run build",
    "preversion": "yarn run lint",
    "version": "yarn run format && git add -A src",
    "postversion": "git push && git push --tags",
    "start": "yarn run build && node ./lib/index.js",
    "test": "yarn run build && node ./lib/test.js",
    "exp-airdrop-2": "yarn run build && node ./lib/exp-airdrop-2.js",
    "send-exp-tokens": "yarn run build && node ./lib/send-exp-tokens.js",
    "send-exp-tokens-bulk": "yarn run build && node ./lib/send-exp-tokens-bulk.js",
    "send-tio-tokens": "yarn run build && node ./lib/send-tio-tokens.js",
    "send-tio-tokens-single": "yarn run build && node ./lib/send-tio-tokens-single.js",
    "get-ao-balances": "yarn run build && node ./lib/get-ao-balances.js",
    "get-exp-balances": "yarn run build && node ./lib/get-exp-balances.js",
    "get-ario-balances": "yarn run build && node ./lib/get-ario-balances.js",
    "get-paginated-exp-balances": "yarn run build && node ./lib/get-paginated-exp-balances.js",
    "get-tio-balances": "yarn run build && node ./lib/get-tio-balances.js",
    "get-dio-balances": "yarn run build && node ./lib/get-dio-balances.js",
    "get-ardrive-balances": "yarn run build && node ./lib/get-ardrive-balances.js",
    "get-ao-balance": "yarn run build && node ./lib/get-ao-balance.js",
    "get-network-details": "yarn run build && node ./lib/get-network-details.js",
    "score-token-holders": "yarn run build && node ./lib/exp-airdrop-2/scoreTokenHolders.js",
    "score-arns-records": "yarn run build && node ./lib/exp-airdrop-2/scoreArNSRecords.js",
    "score-gateways": "yarn run build && node ./lib/exp-airdrop-2/scoreGateways.js",
    "score-delegates": "yarn run build && node ./lib/exp-airdrop-2/scoreDelegates.js",
    "score-primary-names": "yarn run build && node ./lib/exp-airdrop-2/scorePrimaryNames.js",
    "score-protocol-land": "yarn run build && node ./lib/exp-airdrop-2/scoreProtocolLandUsers.js",
    "score-permaweb-deploy": "yarn run build && node ./lib/exp-airdrop-2/scorePermawebDeployUsers.js",
    "score-vouched-users": "yarn run build && node ./lib/exp-airdrop-2/scoreVouchedUsers.js",
    "score-paragraph-users": "yarn run build && node ./lib/exp-airdrop-2/scoreParagraphUsers.js",
    "score-mirror-users": "yarn run build && node ./lib/exp-airdrop-2/scoreMirrorUsers.js",
    "merge-scores": "yarn run build && node ./lib/exp-airdrop-2/mergeScores.js",
    "score-wallet-age": "yarn run build && node ./lib/exp-airdrop-2/scoreWalletAge.js",
    "calculate-final-exp-balances": "yarn run build && node ./lib/exp-airdrop-2/calculateFinalEXPBalances.js",
    "convert-final-scores": "yarn run build && node ./lib/exp-airdrop-2/convertFinalScoresToJSON.js"
  },
  "lint-staged": {
    "**/*.{ts,js,json}": [
      "eslint --fix . --ext .ts",
      "prettier --write ."
    ]
  },
  "packageManager": "yarn@3.6.4",
  "license": "GPL-3.0-or-later",
  "files": [
    "lib/**/*"
  ]
}
